var AuctionDeployed = abi.new({from:eth.accounts[0], data:bytecode,gas:2000000})

**
miner.setEtherbase(eth.accounts[0])
miner.start(1)
miner.stop()
**

var AuctionInst = abi.at(AuctionDeployed.address)


